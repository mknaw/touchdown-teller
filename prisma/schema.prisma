generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DB_PATH")
}

model Game {
  id   Int    @id @default(autoincrement())
  week Int
  home String
  away String
}

model Player {
  id       Int    @id @default(autoincrement())
  adp      Int
  team     String
  position String
  name     String
}
